<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="de.bht.fpa.mail.s797307.imapnavigation.ImapNavigationView"
            id="de.bht.fpa.mail.s797307.imapnavigation.ImapNavigationView"
            name="ImapNavigationView"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="*">
         <view
               id="de.bht.fpa.mail.s797307.imapnavigation.ImapNavigationView"
               minimized="false"
               relationship="stack"
               relative="de.bht.fpa.mail.s797307.main.perspective.left">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="de.bht.fpa.mail.s797307.imapnavigation.commands.category"
            name="Basic Menu Commands">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="de.bht.fpa.mail.s797307.imapnavigation.handlers.DirectoryHandler"
            commandId="de.bht.fpa.mail.s797307.imapnavigation.commands.openDirectory">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
   </extension>
</plugin>
